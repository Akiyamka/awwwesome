@import './../../../colorTheme'

svgFirstColor = secondColor
svgSecondColor = mainColor

.logo-main
  fill: svgSecondColor
.logo-second
  fill: svgFirstColor

.cls-wide
.cls-line
.cls-line-secondary
.cls-line3
.cls-shortDash
.cls-longDash
  stroke: svgFirstColor
  fill: none

.cls-line
.cls-line3
.cls-line-secondary
  fill: none
  stroke-miterlimit: 10
  stroke-width: 1px
  stroke-dasharray: 2000
  stroke-dashoffset: 2000
  stroke-linejoin: round

.cls-line-secondary
  opacity: 1

.cls-rect
.cls-filled
  opacity: 0
  fill: none
  stroke: svgFirstColor
  stroke-width: 1px

.cls-wide
  stroke-width: 8px
  opacity: 0
  stroke: svgFirstColor

.cls-shortDash
  stroke-dasharray: 2.01 2.01
  opacity: 0

.cls-longDash
  stroke-linecap: round
  stroke-dasharray: 12.23 12.23
  opacity: 0

.cls-glow
  fill: none
  stroke: svgFirstColor
  stroke-width: 4px
  opacity: 0
  box-shadow: 0px 0px 8px 0px svgFirstColor
  -webkit-filter: drop-shadow( 0px 0px 8px 0px svgFirstColor )
  filter: drop-shadow( 0px 0px 8px 0px svgFirstColor )

.cls-glow-img
  opacity: 0